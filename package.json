{
  "name": "react-dynamic-forms",
  "version": "0.15.0",
  "description": "Dynamic forms library for React",
  "license": "BSD-3-Clause-LBNL",
  "main": "index.js",
  "author": "ESnet Tools Team <tools@es.net>",
  "repository": {
    "type": "git",
    "url": "git://github.com:esnet/esnet-react-forms.git"
  },
  "bugs": {
    "url": "https://github.com/esnet/esnet-react-forms/issues"
  },
  "scripts": {
    "docs": "echo \"*** Building API docs\n\" && react-docgen src/components -x js -o src/website/api/docs.json --pretty",
    "lint": "eslint src/components/*.js",
    "test": "npm run lint",
    "build": "echo \"*** Building lib\n\" && rm -rf lib/* && babel src/components --optional runtime --stage 0 --out-dir lib/components && babel src/js --optional runtime --stage 0 --out-dir lib/js && babel src/entry.js --optional runtime --stage 0 --out-file lib/entry.js",
    "start-website": "react-scripts start",
    "build-website": "echo \"*** Building website\n\" && rm -rf docs && react-scripts build && mv build docs"
  },
  "pre-commit": [
    "lint",
    "build"
  ],
  "dependencies": {
    "classnames": "^2.1.3",
    "deepcopy": "^0.5.0",
    "invariant": "^2.1.1",
    "jquery": "^2.1.1",
    "moment": "^2.10.6",
    "react-addons-css-transition-group": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-datepicker": "^0.39.0",
    "react-select": "^1.0.0-beta14",
    "revalidator": "^0.3.1",
    "string-hash": "^1.1.0",
    "underscore": "^1.7.0"
  },
  "peerDependencies": {
    "react": "^15.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "bundle-loader": "^0.5.0",
    "chance": "^0.7.7",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-config-esnet": "^0.1.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-flowtype": "^2.17.1",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "file-loader": "^0.7.2",
    "react": "^15.4.2",
    "react-docgen": "^2.9.1",
    "react-dom": "^15.4.2",
    "react-markdown": "^1.0.5",
    "react-router": "^2.6.1",
    "react-scripts": "0.2.1",
    "style-loader": "^0.8.0",
    "url-loader": "^0.5.5",
    "webpack-dev-server": "^1.6.5"
  },
  "keywords": [
    "forms",
    "dynamic",
    "react"
  ]
}
